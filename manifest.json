{
    "manifest_version": 2,

    "name": "Metro Tab",
    "description": "A simple and slick speed dial (new tab page) for Chrome browser.",
    "version": "1.0",

    "chrome_url_overrides":{
        "newtab": "index.html"
    },

    "browser_action":{
        "default_icon": "img/favicon.png"
    },

    "permissions":[
        "tabs"
    ]
}